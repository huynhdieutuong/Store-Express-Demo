<h1>{{title}}</h1>
{{#if arrCart}}
<table class="table">
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Price</th>
      <th>Quantity</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    {{#each arrCart}}
    <tr>
      <td>
        <img class="object-fit-cover" src={{#mainImage this.images 0}}{{/mainImage}} width="100" height="60">
      </td>
      <td>{{this.name}}</td>
      <td>{{this.price}}</td>
      <td>{{this.quantity}}</td>
      <td>{{this.total}}</td>
    </tr>
    {{/each}}
    <tr>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td>{{totalMoney}}</td>
    </tr>
  </tbody>
</table>
{{else}}
<p>No product in cart</p>
{{/if}}